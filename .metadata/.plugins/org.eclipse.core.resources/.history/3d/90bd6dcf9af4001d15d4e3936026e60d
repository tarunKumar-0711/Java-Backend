package javaPractice;

public class StaticUtility {
	String name;
	int roll;
	
	static String uname;
	static int counter=0;
	
	public StaticUtility(String name) {
		this.name=name;
		this.roll =++counter;
	}
	static 
	public static void main(String args[]) {
		StaticUtility tarun = new StaticUtility();
		
	}
}
