package com.Collections;
import java.util.*;

public class ArrayListNew {
	public static void main(String args[]) {
		List<Integer> lst = new ArrayList<>();
		lst.add(1);
		lst.add(2);
		lst.add(57);
		lst.add(38);
		lst.add(7);
		lst.add(0);
		System.out.println(lst);
		lst.add(4,80);
		System.out.println(lst);
		lst.set(2, 30);
		System.out.println(lst);
		lst.remove(Integer.valueOf(38));//idx
		System.out.println(lst);
		System.out.println(lst.get(0));
	}
}
