package com.Map;

import java.util.WeakHashMap;

class Testing {

	@Override
	public String toString() {
		return "Key";
	}

}

public class Weak {

	public static void main(String[] args) {
		Testing testing = new Testing();
		WeakHashMap<Object, String> map = new WeakHashMap<>();
		map.put(testing, "HelloWorld!!");
	}
}